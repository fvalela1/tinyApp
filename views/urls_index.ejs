<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>

<header>
<% include ./partials/_header %>
</header>


	<ul>
		<% for(let key in urls){%>
			<li><a href="/urls/<%= key %>"> <%= key %> </a> : <%= urls[key] %>  
				<form method="POST" action="/urls/<%= key %>/delete"><button type="submit">Delete</button></form>
			</li>

		<% } %>
   		 
   		 <p><a href="/urls/new"> Make A New Short URL! </a></p>
  	</ul>



</body>
</html>